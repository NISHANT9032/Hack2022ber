#include <iostream>  
using namespace std;  
#include<math.h>
int main()  
{  
   unsigned long long int i,fact=1,number,x,result;    
     cin>>x;
 cin>>number; 
 if(number<=0)
 return 1;
 else {   
  for(i=1;i<=number;i++) 
      fact=fact*i;    
  }    
  result=pow(x,fact%10);
  cout<<result%10;
    
  return 0;  
}  
